{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettify": "prettier --write './src/**/*.js'",
    "lint": "eslint ./src/**/*.js"
  },
  "proxy": "http://localhost:4000",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "lint": "^1.1.2",
    "prettier": "^1.7.4"
  },
  "prettier": {
    "single-quote": true,
    "tab-width": 2
  },
  "eslintConfig": {
    "env": {
      "node": false,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": 1
    }
  }
}
